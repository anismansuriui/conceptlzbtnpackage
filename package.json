    "autoprefixer": "10.0.4",
    "postcss-cli": "8.3.0",
    "postcss-import": "13.0.0",
    "postcss-nested": "5.0.1",
    "prejss-cli": "0.3.3",
    "prettier": "^2.7.1"
  },
